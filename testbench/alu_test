#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55b26549b490 .scope module, "alu_tb" "alu_tb" 2 2;
 .timescale -9 -12;
v0x55b2654e9970_0 .var "a", 31 0;
v0x55b2654e9a50_0 .var "b", 31 0;
v0x55b2654e9b20_0 .var "op", 2 0;
v0x55b2654e9c20_0 .net "result", 31 0, v0x55b2654e97e0_0;  1 drivers
S_0x55b26549b620 .scope module, "uut" "alu" 2 8, 3 2 0, S_0x55b26549b490;
 .timescale -9 -12;
    .port_info 0 /INPUT 32 "a";
    .port_info 1 /INPUT 32 "b";
    .port_info 2 /INPUT 3 "op";
    .port_info 3 /OUTPUT 32 "result";
v0x55b2654d6360_0 .net "a", 31 0, v0x55b2654e9970_0;  1 drivers
v0x55b2654e9610_0 .net "b", 31 0, v0x55b2654e9a50_0;  1 drivers
v0x55b2654e96f0_0 .net "op", 2 0, v0x55b2654e9b20_0;  1 drivers
v0x55b2654e97e0_0 .var "result", 31 0;
E_0x55b2654d6b70 .event anyedge, v0x55b2654e96f0_0, v0x55b2654d6360_0, v0x55b2654e9610_0;
    .scope S_0x55b26549b620;
T_0 ;
    %wait E_0x55b2654d6b70;
    %load/vec4 v0x55b2654e96f0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55b2654e97e0_0, 0, 32;
    %jmp T_0.6;
T_0.0 ;
    %load/vec4 v0x55b2654d6360_0;
    %load/vec4 v0x55b2654e9610_0;
    %add;
    %store/vec4 v0x55b2654e97e0_0, 0, 32;
    %jmp T_0.6;
T_0.1 ;
    %load/vec4 v0x55b2654d6360_0;
    %load/vec4 v0x55b2654e9610_0;
    %sub;
    %store/vec4 v0x55b2654e97e0_0, 0, 32;
    %jmp T_0.6;
T_0.2 ;
    %load/vec4 v0x55b2654d6360_0;
    %load/vec4 v0x55b2654e9610_0;
    %and;
    %store/vec4 v0x55b2654e97e0_0, 0, 32;
    %jmp T_0.6;
T_0.3 ;
    %load/vec4 v0x55b2654d6360_0;
    %load/vec4 v0x55b2654e9610_0;
    %or;
    %store/vec4 v0x55b2654e97e0_0, 0, 32;
    %jmp T_0.6;
T_0.4 ;
    %load/vec4 v0x55b2654d6360_0;
    %load/vec4 v0x55b2654e9610_0;
    %xor;
    %store/vec4 v0x55b2654e97e0_0, 0, 32;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55b26549b490;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "alu.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55b26549b490 {0 0 0};
    %vpi_call 2 13 "$display", "Simulation started" {0 0 0};
    %pushi/vec4 15, 0, 32;
    %store/vec4 v0x55b2654e9970_0, 0, 32;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x55b2654e9a50_0, 0, 32;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x55b2654e9b20_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x55b2654e9b20_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x55b2654e9b20_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x55b2654e9b20_0, 0, 3;
    %delay 10000, 0;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x55b2654e9b20_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_1;
